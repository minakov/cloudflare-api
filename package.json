  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild --bundle --sourcemap --outdir=dist ./src/index.js",
    "dev": "miniflare --watch --debug",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@types/itty-router-extras": "^0.4.0",
